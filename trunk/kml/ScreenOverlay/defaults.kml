<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.1">
<Document>
  <name>ScreenOverlay defaults</name>
  <open>1</open>

  <ScreenOverlay>
    <!--
    Empty elements given. Default values are implied. Draws a square white box
    in the lower left corner of the screen. (Note the sheer brilliance of
    drawing a polygon where an icon is not supplied.)
    -->
    <name>All defaults</name>
    <visibility>0</visibility>
    <Icon>
      <href/>
    </Icon>
    <overlayXY/>
    <screenXY/>
    <rotationXY/>
    <size/>
  </ScreenOverlay>

  <ScreenOverlay>
    <!--
    Defaults for elements are stated explicitly. Also draws a square white box
    in the lower left corner of the screen.
    -->
    <name>Explicit defaults</name>
    <visibility>0</visibility>
    <Icon>
      <href/>
    </Icon>
    <!-- lower-left corner of image -->
    <overlayXY x="0" y="0" xunits="pixels" yunits="pixels" />
    <!-- placed at lower-left corner of screen -->
    <screenXY x="0" y="0" xunits="pixels" yunits="pixels" />
    <!-- rotation, if supplied, will be around center of screen -->
    <rotationXY x="0.5" y="0.5" xunits="pixels" yunits="pixels" />
    <!-- and display the image at its native pixel resolution -->
    <size x="-1" y="-1" xunits="pixels" yunits="pixels" />
  </ScreenOverlay>

</Document>
</kml>
